<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

    <context id="mysql_tables" targetRuntime="MyBatis3">
        <!-- 防止生成的代码中有很多注释，加入下面的配置控制 -->
        <commentGenerator>
            <property name="suppressDate" value="true" />
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true" />
        </commentGenerator>

        <jdbcConnection connectionURL="jdbc:mysql://127.0.0.1:3306/cinemaboot?serverTimezone=GMT%2B8"
                        driverClass="com.mysql.jdbc.Driver"
                        userId="root"
                        password="123456">
            <property name="nullCatalogMeansCurrent" value="true"/>
        </jdbcConnection>

        <javaModelGenerator targetPackage="com.loststars.cinemaboot.movie.dataobject" targetProject="C:\\Users\\Intel\\IdeaProjects\\cinemaboot\\cinemaboot-movie\\src\\main\\java" >
            <property name="enableSubPackages" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mapper" targetProject="C:\\Users\\Intel\\IdeaProjects\\cinemaboot\\cinemaboot-movie/src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <javaClientGenerator targetPackage="com.loststars.cinemaboot.movie.dao" targetProject="C:\\Users\\Intel\\IdeaProjects\\cinemaboot\\cinemaboot-movie\\src\\main\\java" type="XMLMAPPER" >
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <!--<table tableName="movie" domainObjectName="MovieDO">-->
            <!--<property name="useActualColumnNames" value="false"/>-->
        <!--</table>-->

        <table tableName="sales_log" domainObjectName="SalesLogDO">
            <property name="useActualColumnNames" value="false"/>
        </table>

        </context>
    </generatorConfiguration>